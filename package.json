{
  "name": "sneeze-log-server",
  "author": "James Smith",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server:prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "heroku-postbuild": "cross-env NODE_ENV=production & npm-run-all clean build",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "nodemailer": "^6.3.0",
    "pg": "^8.11.3",
    "randomatic": "^3.1.1",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-plugin-node": "^8.0.1",
    "make-runnable": "^1.3.6",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5"
  }
}
